
insert into qhms.qmain_pxprofile_pri(`pxpidrs`) select id from his.dprofilpasien where left(id,1)='0';
update qhms.qmain_pxprofile_pri,his.dprofilpasien set qhms.qmain_pxprofile_pri.pxpupdate=his.dprofilpasien.update,qhms.qmain_pxprofile_pri.pxpnama=CamelCase(his.dprofilpasien.firstname),qhms.qmain_pxprofile_pri.pxpalamat=Camelcase(his.dprofilpasien.alamat),qhms.qmain_pxprofile_pri.pxptelp=his.dprofilpasien.telp,qhms.qmain_pxprofile_pri.pxphp=his.dprofilpasien.hp,qhms.qmain_pxprofile_pri.pxptplhr=Camelcase(his.dprofilpasien.tempatlahir),qhms.qmain_pxprofile_pri.pxptglhr=his.dprofilpasien.tgllahir,qhms.qmain_pxprofile_pri.pxpdes=(select qvar_desa.id from qvar_desa join his.dprofilpasien on lower(trim(his.dprofilpasien.kelurahan))=lower(trim(qvar_desa.namades)) where lower(trim(his.dprofilpasien.kelurahan))=lower(trim(qvar_desa.namades)) limit 1),qhms.qmain_pxprofile_pri.pxpkec=(select qvar_kec.id from qvar_kec join his.dprofilpasien on lower(trim(his.dprofilpasien.kecamatan))=lower(trim(qvar_kec.namakec)) where lower(trim(his.dprofilpasien.kecamatan))=lower(trim(qvar_kec.namakec)) limit 1),qhms.qmain_pxprofile_pri.pxpkab=(select qvar_kab.id from qvar_kab join his.dprofilpasien on lower(trim(his.dprofilpasien.kabupaten))=lower(trim(qvar_kab.namakab)) where lower(trim(his.dprofilpasien.kabupaten))=lower(trim(qvar_kab.namakab)) limit 1),qhms.qmain_pxprofile_pri.pxpprp=(select qvar_prop.id from qvar_prop join his.dprofilpasien on lower(trim(his.dprofilpasien.propinsi))=lower(trim(qvar_prop.namaprp)) where lower(trim(his.dprofilpasien.propinsi))=lower(trim(qvar_prop.namaprp)) limit 1),qhms.qmain_pxprofile_pri.pxpneg=(select qvar_negara.varid from qvar_negara join his.dprofilpasien on lower(trim(his.dprofilpasien.negara))=lower(trim(qvar_negara.varnama)) where lower(trim(his.dprofilpasien.negara))=lower(trim(qvar_negara.varnama)) limit 1),qhms.qmain_pxprofile_pri.pxpdik=(select qvar_pendidikan.varid from qvar_pendidikan join his.dprofilpasien on lower(trim(his.dprofilpasien.pendidikan))=lower(trim(qvar_pendidikan.varnama)) where lower(trim(his.dprofilpasien.pendidikan))=lower(trim(qvar_pendidikan.varnama)) limit 1),qhms.qmain_pxprofile_pri.pxpkrj=(select qvar_pekerjaan.varid from qvar_pekerjaan join his.dprofilpasien on lower(trim(his.dprofilpasien.pekerjaan))=lower(trim(qvar_pekerjaan.varnama)) where lower(trim(his.dprofilpasien.pekerjaan))=lower(trim(qvar_pekerjaan.varnama)) limit 1),qhms.qmain_pxprofile_pri.pxpstskw=(select qvar_umum.varid from qvar_umum join his.dprofilpasien on lower(trim(his.dprofilpasien.status))=lower(trim(qvar_umum.varnama)) where lower(trim(his.dprofilpasien.status))=lower(trim(qvar_umum.varnama)) limit 1),qhms.qmain_pxprofile_pri.pxpkunjungan=(select qvar_umum.varid from qvar_umum join his.dprofilpasien on lower(trim(his.dprofilpasien.kunjungan))=lower(trim(qvar_umum.varnama)) where lower(trim(his.dprofilpasien.kunjungan))=lower(trim(qvar_umum.varnama)) limit 1) where qhms.qmain_pxprofile_pri.pxpidrs=his.dprofilpasien.id;
insert ignore into qhms.qmain_pxprofile_kel(`pxkidrs`) select id from his.dprofilpasien where left(id,1)='0' and namaayah<>'';
insert ignore into qhms.qmain_pxprofile_kel(`pxkidrs`) select id from his.dprofilpasien where left(id,1)='0' and namaibu<>'';
insert ignore into qhms.qmain_pxprofile_kel(`pxkidrs`) select id from his.dprofilpasien where left(id,1)='0' and peksuami<>'';
insert ignore into qhms.qmain_pxprofile_kel(`pxkidrs`) select id from his.dprofilpasien where left(id,1)='0' and umsuami<>'';
update qhms.qmain_pxprofile_kel,his.dprofilpasien set qhms.qmain_pxprofile_kel.pxkupsgn=his.dprofilpasien.umsuami, qhms.qmain_pxprofile_kel.pxknmayah=his.dprofilpasien.namaayah, qhms.qmain_pxprofile_kel.pxknmibu=his.dprofilpasien.namaibu where qhms.qmain_pxprofile_kel.pxkidrs=his.dprofilpasien.id;
insert into qhms.qmain_pxprofile_tgg(`pxtidrs`) select id from his.dpenanggung where left(id,1)='0' and penanggung<>'';
insert ignore into qhms.qmain_pxprofile_tgg(`pxtidrs`) select id from his.dpenanggung where left(id,1)='0' and alamat<>'';
insert ignore into qhms.qmain_pxprofile_tgg(`pxtidrs`) select id from his.dpenanggung where left(id,1)='0' and pekerjaan<>'';
insert ignore into qhms.qmain_pxprofile_tgg(`pxtidrs`) select id from his.dpenanggung where left(id,1)='0' and telp<>'';
update qhms.qmain_pxprofile_tgg,his.dpenanggung set qhms.qmain_pxprofile_tgg.pxtnmtgg=his.dpenanggung.penanggung,qhms.qmain_pxprofile_tgg.pxtalamat=his.dpenanggung.alamat,qhms.qmain_pxprofile_tgg.pxtalamat=his.dpenanggung.alamat where qhms.qmain_pxprofile_tgg.pxtidrs=his.dpenanggung.id;
